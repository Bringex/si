полная дичь

#include <stdio.h>
#include <math.h>
#include <malloc.h>

int len_str(char *str){
    int i = 0;
    while(str[++i] != '\0');
    return i;
}

char *strcat(char *str1, char *str2){
  
    int lenStr = len_str(str1) + len_str(str2);
    char *new_str = (char*)malloc(lenStr * sizeof(char));
    int counter, i = 0;
    
    for (i = 0; i < len_str(str1); i++)
        new_str[i] = str1[i];
    counter = i;
    for (i = 0; i < len_str(str2); i++)
        new_str[counter + i] = str2[i];
    
    return new_str;
}
char *strcpy(char *str1, char *str2){
  
    int lenStr = len_str(str1);
    char *new_str = (char*)malloc(lenStr * sizeof(char));
    for (int i = 0; i < lenStr; i++)
        if (str2[i] != '\0')
            new_str[i] = str2[i];
        else
            new_str[i] = '\0';

    return new_str;
}

int strncmp(char *str1, char *str2, size_t n){
    
    for (int i = 0; i < n; i++)
        if ( str1[i] != '\0' && str2[i] != '\0')
            if (str1[i] == str2[i] && (i - 1) == n)
                 return 0;
            else if (str1[i] != str2[i])
                 return str1[i] - str2[i];
        else 
            return 0;

}


int main()
{
  //18  1,  4, 5, 8, 11

  char str1[20];
  char str2[20];
  scanf("%s", &str1);
  scanf("%s", &str2);
  printf("concat string: %s\n", strcat(str1, str2));
  printf("copy string: %s\n", strcpy(str1, str2));
  printf("copy string: %s\n", strncmp(str1, str2, 5));
  return 0;
}
