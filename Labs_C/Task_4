
#include <stdio.h>
#include <math.h>

int main()
{
  long a;
  long long b;
  printf("Write number: ");
  scanf("%d", &a);
  printf("Exist in interval: %d \n", a >= -77 && a <= -1);
  
  printf("Write number: ");
  scanf("%x", &b);
  // 0x40000 = 0100 0000 0000 0000
  printf("18 bit is: %d ", (b >> 18) & 1);
  getchar();
  return 0;
}
