#include <stdio.h>
#include <math.h>

#define PI acos(-1.0)

int main()
{
  //лаба 2 вариант 1
  double a;
  double first_answer;
  double second_answer;

  printf("Write a: ");
  scanf("%lf", &a);

  if (a != 0 && a != 2){
    first_answer = ((a + 2) / sqrt(2 * a) - a / (sqrt(2 * a) + 2) + 2 / (a - sqrt(2 * a))) * (sqrt(a) - sqrt(2)) / (a + 2);
    second_answer = 1 / (sqrt(a) + sqrt(2));

    printf("Z1: %lf, Z2: %lf", first_answer, second_answer);
  }
  else
    printf("Error");
  
  getchar();
  return 0;
}
